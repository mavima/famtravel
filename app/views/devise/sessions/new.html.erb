  <div class="login-form-our-famtravel-app">
  <h2>Log in</h2>
  <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="form-inputs session-form">
      <%= f.input :email,
                  required: false,
                  autofocus: true,
                  label_html:  { class: "email_field" }%>
      <%= f.input :password,
                  required: false,
                  label_html:  { class: "password_field"}  %>
      <%= f.input :remember_me, wrapper_html: { class: "remember_me_part" }, as: :boolean if devise_mapping.rememberable? %>
      <%= f.button :submit, class: "two_last_buttons" %>
      <%= render "devise/shared/links" %>
      <% end %>
      </div>
    </div>



